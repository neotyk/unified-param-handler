{
  "name": "unified-param-handler",
  "version": "1.2.0",
  "description": "Handles URL parameters and cookies, updating hidden fields.",
  "main": "dist/unified-handler.js",
  "module": "dist/unified-handler.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js tests/**/*.js",
    "build:dev": "webpack --mode development --env development",
    "build": "webpack --mode production --env production",
    "watch": "webpack --watch --mode development --env development",
    "prepare": "husky",
    "release": "standard-version"
  },
  "keywords": [
    "utm",
    "gclid",
    "fbc",
    "fbp",
    "parameters",
    "cookies",
    "tracking"
  ],
  "author": "Alex Iwaniuk <alex@alexiwaniuk.com>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^2.8.0",
    "standard-version": "^9.5.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": false
    },
    "commitAll": true,
    "types": [
      {"type": "feat", "section": "Features"},
      {"type": "fix", "section": "Bug Fixes"},
      {"type": "chore", "hidden": true},
      {"type": "docs", "hidden": true},
      {"type": "style", "hidden": true},
      {"type": "refactor", "hidden": true},
      {"type": "perf", "section": "Performance Improvements"},
      {"type": "test", "hidden": true}
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
